Algoritmo "eu_tenho_a_força"
Var
   valor: Inteiro

Inicio
   Escreva("Digite um valor: ")
   Leia(valor)

   Se (valor >= 30) Entao
      Escreva("Preciso de cálcio")
   Senao
      Se (valor >= 20) Entao
         Escreva("Para o alto e além")
      Senao
         Se (valor >= 10) Entao
            Escreva("Eu tenho a força")
         FimSe
      FimSe
   FimSe
Fimalgoritmo
